<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.marfy.memoryleaks">

  <uses-permission android:name="android.permission.INTERNET"/>

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:name=".App"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity
      android:label="@string/app_name"
      android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>

    <activity android:name=".statics.StaticActivity"/>
    <activity android:name=".asynctask.AsyncTaskActivity"/>
    <activity android:name=".retrofit.RetrofitActivity"/>
    <activity android:name=".rxjava.RxJavaActivity"/>
    <activity android:name=".handler.HandlerActivity"/>
    <activity android:name=".mvp.MvpActivity"/>
    <activity android:name=".animation.AnimationActivity"/>
    <activity android:name=".fragment.FragmentActivity"/>
  </application>

</manifest>